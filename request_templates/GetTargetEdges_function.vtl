{
  "operation": "Query",
	"query" : {
    "index": "lsi0",
    "expression" : "pk = :tenant AND begins_with(lsi0_sk = :target)",
    "expressionValues" : {
      ":tenant" : $util.dynamodb.toDynamoDBJson($ctx.stash.tenant),
      ":target": $util.dynamodb.toDynamoDBJson("E~$!{ctx.stash.name}|")
    }
  },
  limit: 1
}
